<?xml version='1.0' encoding='utf-8'?>
<widget id="com.riv.Visitar" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="webView" value="org.crosswalk.engine.XWalkWebViewEngine" />
    <preference default="16+" name="xwalkVersion" />
    <preference default="--disable-pull-to-refresh-effect" name="xwalkCommandLine" />
    <preference default="embedded" name="xwalkMode" />
    <preference default="true" name="xwalkMultipleApk" />
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Insomnia">
        <param name="android-package" value="nl.xservices.plugins.Insomnia" />
    </feature>
    <feature name="Zip">
        <param name="android-package" value="org.apache.cordova.Zip" />
    </feature>
    <splash qualifier="land-hdpi" src="landscape-hdpi.png" />
    <splash qualifier="land-ldpi" src="landscape-ldpi.png" />
    <splash qualifier="land-mdpi" src="landscape-mdpi.png" />
    <splash qualifier="land-xhdpi" src="landscape-xhdpi.png" />
    <splash qualifier="land-xxhdpi" src="landscape-xxhdpi.png" />
    <splash qualifier="port-hdpi" src="portrait-hdpi.png" />
    <splash qualifier="port-ldpi" src="portrait-ldpi.png" />
    <splash qualifier="port-mdpi" src="portrait-mdpi.png" />
    <splash qualifier="port-xhdpi" src="portrait-xhdpi.png" />
    <splash qualifier="port-xxhdpi" src="portrait-xxhdpi.png" />
    <name>Visitar</name>
    <description>
        An app for viewing AR and VR tours.
    </description>
    <author email="contact@revolutioninteractivevideo.com" href="http://revolutioninteractivevideo.com">
        Revolution Interactive Video
    </author>
    <content src="index.html" />
    <icon src="www/img/logo.png" />
    <access origin="http://revolutioninteractivevideo.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <splash src="splash.png" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="6800" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="xwalkVersion" value="16+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
</widget>

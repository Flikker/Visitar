<?xml version='1.0' encoding='utf-8'?>
<widget id="com.riv.Visitar" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Insomnia">
        <param name="ios-package" value="Insomnia" />
    </feature>
    <feature name="Zip">
        <param name="ios-package" value="ZipPlugin" />
    </feature>
    <description>
        An app for viewing AR and VR tours.
    </description>
    <preference name="SplashScreenDelay" value="8000" />
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <splash height="480" src="Default.png" width="320" />
    <splash height="960" src="Default@2x.png" width="640" />
    <splash height="1136" src="Default-568h@2x.png" width="640" />
    <splash height="1334" src="Default-667h@2x.png" width="750" />
    <splash height="2208" src="Default-Portrait-736h@3x.png" width="1242" />
    <splash height="1242" src="Default-Landscape-736h@3x.png" width="2208" />
    <splash height="1024" src="Default-Portrait.png" width="768" />
    <splash height="768" src="Default-Landscape.png" width="1024" />
    <splash height="2048" src="Default-Portrait@2x.png" width="1536" />
    <splash height="1536" src="Default-Landscape@2x.png" width="2048" />
    <name>Visitar</name>
    <description>
        An app for viewing AR and VR tours.
    </description>
    <author email="contact@revolutioninteractivevideo.com" href="http://revolutioninteractivevideo.com">
        Revolution Interactive Video
    </author>
    <content src="index.html" />
    <icon src="www/img/logo.png" />
    <access origin="http://revolutioninteractivevideo.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <splash src="splash.png" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="6800" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="xwalkVersion" value="16+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
</widget>
